            "subtle": {
                "default": {
                    "backgroundColor": "var(--neutral-darker)",
                    "textColor": "var(--neutral-white)"
                },
                "hover": {
                    "backgroundColor": "var(--neutral-dark)",
                    "textColor": "var(--neutral-lightest)"
                },
                "active": {
                    "backgroundColor": "var(--neutral-darker)",
                    "textColor": "var(--neutral-white)"
                },
                "disabled": {
                    "backgroundColor": "var(--neutral-light)",
                    "textColor": "var(--neutral-medium)"
                }
            }
        },
        "typography": {
