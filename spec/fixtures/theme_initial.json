{
    "name": "Unit-Ruby Specs Default Theme",
    "global": {
        "avatar": {
            "iconColor": "var(--primary-main)",
            "borderColor": "var(--secondary-lighten-2)",
            "backgroundColor": "var(--secondary-lighten-5)"
        },
        "logoUrl": "https://app.s.unit.sh/logo-dark.f1c96208.svg",
        "colors": {
            "success": "#93CCBC",
            "background": "#FFFFFF",
            "warning": "#F6D469",
